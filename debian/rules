#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

# to run with quilt use this instead:
# dh  --with quilt $@

%:
	dh $@ --sourcedirectory=build

override_dh_clean:
	dh_clean -O--sourcedirectory=build
	rm -f \
		inc/com/centreon/clib/version.hh \
		script/centreon-clib.pc
